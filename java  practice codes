        0 1 0 1
        1 0 1 0 1
     */

    static void p7() {
        int n = 5;
        int j = 1;
        for (int i = 1; i <= n; i++) {
            if (i % 2 == 0) {
                j = 0;
            } else j = 1;
            for (int k = 1; k <= i; k++) {
                System.out.print(j + " ");
                if (j == 0) {
                    j = 1;
                } else j = 0;

            }System.out.println();
        }
    }

