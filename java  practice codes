 //count negitive numbers .
 

public class CountNegativeNums {
    public static void main(String[] args) {
        int[][] arr= { {4,3,2,-1},
                      {3,2,1,-1},
                       {1,1,-1,-2},
                      {-1,-1,-2,-3}
        };
        int count =0;

        for (int row = 0; row <arr.length; row++) {
            for (int ele:arr[row]) {

                if(ele <0){
                    count++;
                }

            }

        }
        System.out.println(count);

    }
}
