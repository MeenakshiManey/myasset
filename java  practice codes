    static void p16(){
        int n=5;
        for (int row = 1; row <=n; row++) {
            for (int spc = row; spc <n ; spc++) {
                System.out.print(" ");
            }
            for (int col = 0; col <(2 * row)-1; col++) {
                if ( col == 0 || col==((2 * row)-1)-1 ){
                System.out.print("*");}else
                    System.out.print(" ");
            }
            for (int next = row; next <n ; next++) {
                System.out.print(" ");
            }
            System.out.println();
        }
        for (int row = n; row > 0; row--) {
            for (int spc = row; spc <n ; spc++) {
                System.out.print(" ");
            }
            for (int col = 0; col < (2 * row)-1; col++) {
                if ( col == 0 || col==((2 * row)-1)-1 ){
                    System.out.print("*");}
                else
                    System.out.print(" ");

