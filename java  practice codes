public class PeakIndexMountainArray {
    public static void main(String[] args) {
    int []arr = { 0,1,2,4,7,8,5,3,2};
        System.out.println(search(arr));
    }
    static int search(int[] arr) {
        int s=0;
        int e= arr.length-1;
        while(s<e){
            int mid=s+(e-s)/2;
            if(arr[mid] > arr[mid+1]){
               if (arr[mid] < arr[mid-1]) {
                   e = mid-1 ;
               }
//               else {
//                   return arr[mid] ;
//               }
            }
            else {
                s=mid+1;
            }
        }return arr[s];
    }
}






